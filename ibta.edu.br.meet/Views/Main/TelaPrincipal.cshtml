@Styles.Render("~/Content/TelaPrincipal.css")
@if (Model.Count > 0)
{
    foreach (var item in Model)
    {
    <!-- Hover Demo-->
    <div class="column">
        <div class="demo-title"></div>
        <!-- Post-->
        <div class="post-module hover">
            <!-- Thumbnail-->
            <div class="thumbnail">
                <img src="http://br.web.img3.acsta.net/pictures/15/08/05/20/31/056092.jpg" />
            </div>
                        
            <!-- Post Content-->
            <div class="post-content">
                <h1 class="title">@item.Nome</h1>
                <h2 class="sub_title">@item.Login</h2>
                <p class="description">descrição qualquer uma</p>
                <div class="post-meta row">
                    <span class="comments btn btn-success"><a class="glyphicon glyphicon-thumbs-up" href="/Main/atualizarMatchPositivo" id="@item.Id"></a></span>
                    <span class="comments btn btn-danger"><a class="glyphicon glyphicon-thumbs-down" href="/Main/atualizarMatchPositivo" id="@item.Id"> </a></span>
                </div>
            </div>
        </div>
    </div>
    }
}
